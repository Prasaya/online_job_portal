{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2021",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "typeRoots": [
      "packages/api/typings",
      "node_modules/@types",
    ],
    "outDir": "./dist",
    "allowJs": true,
    "checkJs": false,api/middleware /*
    "baseUrl": ".",api/models/*
    "paths": {api/routes/*
      "@middleware/*api/utils/*
        "api/middlewarapi/typings/*
      ],api/*
      "@models/*": [
        "api/models/*"
      ],api/**/*.ts
      "@routes/*": [api/client /**
        "api/routes/*"
      ],
      "@utils/*": [
        "api/utils/*"
      ],
      "@typings/*": [
        "api/typings/*"
      ],
      "@root/*": [
        "api/*"
      ]
    }
  },
  "include": [
    "api/**/*.ts"
    ],
    "exclude": [
      "**/node_modules/**",
      "client/**python/**"
    ]
  }
